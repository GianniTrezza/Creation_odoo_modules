<odoo>
    <data>


        <record id="view_form_roombooking_inherited" model="ir.ui.view">
            <field name="name">roombooking.form.inherited</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Dettagli Prenotazione">
                        <group>
                            <group>
                                
                                <!-- <field name="refer"/> -->
                                <!-- <field name="refer"/> -->
                                <field name="checkin"/>
                                <field name="checkout"/>
                                <field name="totalChildren"/>
                                <field name="totalInfants"/>
                                <field name="totalGuest"/>
                                <field name="roomGross"/>
                                <!-- <field name="totalGross"/> -->
        
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>


